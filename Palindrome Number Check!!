#include <stdio.h>
#include <stdlib.h> // for abs()

int main() {
    int N;
    scanf("%d", &N);

    int original = N;
    int reversed = 0;

    // Make N positive to handle negative inputs (optional)
    N = abs(N);

    while (N > 0) {
        reversed = reversed * 10 + N % 10;
        N /= 10;
    }

    // Check if original and reversed are the same
    if (abs(original) == reversed)
        printf("Palindrome\n");
    else
        printf("Not Palindrome\n");

    return 0;
}
