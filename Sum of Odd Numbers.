#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);  // Input

    int sum = 0;

    // Loop through all numbers from 1 to N
    for (int i = 1; i <= N; i++) {
        if (i % 2 != 0) {  // Check if the number is odd
            sum += i;
        }
    }

    printf("%d\n", sum);  // Output

    return 0;
}
