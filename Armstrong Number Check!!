#include <stdio.h>
#include <math.h>

int main() {
    int N;
    scanf("%d", &N);

    int original = N;
    int count = 0;
    int temp = N;

    // Count the number of digits
    while (temp != 0) {
        count++;
        temp /= 10;
    }

    int sum = 0;
    temp = N;

    // Calculate sum of digits raised to the power count
    while (temp != 0) {
        int digit = temp % 10;
        sum += pow(digit, count);
        temp /= 10;
    }

    if (sum == original)
        printf("Armstrong\n");
    else
        printf("Not Armstrong\n");

    return 0;
}
