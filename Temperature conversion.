#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {
    float celsius, fahrenheit;

    // Read input in Celsius
    scanf("%f", &celsius);

    // Convert Celsius to Fahrenheit
    fahrenheit = (celsius * 9 / 5) + 32;

    // Print result up to 2 decimal places
    printf("%.2f\n", fahrenheit);

    return 0;
}
