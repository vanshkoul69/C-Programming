#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int isNegative = 0;
    if (N < 0) {
        isNegative = 1;
        N = -N; // Convert to positive for processing
    }

    int reversed = 0;
    while (N > 0) {
        reversed = reversed * 10 + N % 10;
        N /= 10;
    }

    if (isNegative)
        reversed = -reversed;

    printf("%d\n", reversed);

    return 0;
}
