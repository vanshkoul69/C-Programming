#include <stdio.h>

int main() {
    int physics, chemistry, biology, mathematics, computer;
    float percentage;
    char grade;

    // Read marks for five subjects
    scanf("%d %d %d %d %d", &physics, &chemistry, &biology, &mathematics, &computer);

    // Calculate total marks and percentage
    int total = physics + chemistry + biology + mathematics + computer;
    percentage = (total / 500.0f) * 100.0f;

    // Determine grade based on percentage
    if (percentage >= 90.0f) {
        grade = 'A';
    } else if (percentage >= 80.0f) {
        grade = 'B';
    } else if (percentage >= 70.0f) {
        grade = 'C';
    } else if (percentage >= 60.0f) {
        grade = 'D';
    } else if (percentage >= 40.0f) {
        grade = 'E';
    } else {
        grade = 'F';
    }

    // Print results with 2 decimal places for percentage
    printf("Percentage = %.2f\n", percentage);
    printf("Grade = %c\n", grade);

    return 0;
}
