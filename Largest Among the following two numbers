#include <stdio.h>

int main() {
    int x, y;
    

    printf("Enter two numbers (x and y): ");
    
 
    if (scanf("%d %d", &x, &y) != 2) {
        printf("Error: Please enter valid integers.\n");
        return 1; 
    }
    
  
    printf("%s\n", (x > y) ? "x is larger" : (x < y) ? "y is larger" : "x and y are equal");
    
    return 0;
}
