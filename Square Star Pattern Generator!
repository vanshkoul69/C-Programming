#include <stdio.h>

int main() {
    int N;

    // Read input
    scanf("%d", &N);

    // Loop through each row
    for(int i = 0; i < N; i++) {
        // Loop through each column
        for(int j = 0; j < N; j++) {
            printf("* ");
        }
        // Move to the next line after each row
        printf("\n");
    }

    return 0;
}
