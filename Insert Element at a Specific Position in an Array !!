#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[101];  // max size 100 + 1 for insertion

    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int Value, Position;
    scanf("%d", &Value);
    scanf("%d", &Position);

    // Shift elements to the right starting from the last element
    for (int i = N; i >= Position; i--) {
        arr[i] = arr[i - 1];
    }

    // Insert the value at the specified position (1-based index)
    arr[Position - 1] = Value;

    // Print the updated array
    for (int i = 0; i <= N; i++) {
        printf("%d", arr[i]);
        if (i != N) printf(" ");
    }
    printf("\n");

    return 0;
}
