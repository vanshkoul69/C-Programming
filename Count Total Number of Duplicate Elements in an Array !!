#include <stdio.h>

int main() {
    int N;
    
    // Read number of elements
    scanf("%d", &N);
    
    int arr[N];
    
    // Read array elements
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int visited[N];
    for (int i = 0; i < N; i++) {
        visited[i] = 0;  // Mark all as unvisited
    }

    int duplicateCount = 0;

    // Count duplicate elements
    for (int i = 0; i < N; i++) {
        if (visited[i] == 1)
            continue;  // Skip if already checked
        
        int count = 1;
        for (int j = i + 1; j < N; j++) {
            if (arr[i] == arr[j]) {
                count++;
                visited[j] = 1; // Mark as visited
            }
        }
        if (count > 1) {
            duplicateCount++;  // Count only once for each duplicated element
        }
    }

    // Print result
    printf("%d", duplicateCount);
    
    return 0;
}
