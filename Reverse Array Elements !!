#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[50];  // Max size as per constraints

    // Read N elements
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    // Print elements in reverse order
    for (int i = N - 1; i >= 0; i--) {
        printf("%d ", arr[i]);
    }

    return 0;
}
