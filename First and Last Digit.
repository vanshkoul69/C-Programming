#include <stdio.h>
#include <stdlib.h> // for abs()

int main() {
    int N;
    scanf("%d", &N);

    N = abs(N); // Handle negative numbers

    int last_digit = N % 10;

    int first_digit = N;
    while (first_digit >= 10) {
        first_digit /= 10;
    }

    printf("%d %d\n", first_digit, last_digit);

    return 0;
}
